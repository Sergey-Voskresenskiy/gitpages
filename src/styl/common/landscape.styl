.landscape
	display none
	.banana
		margin-top 15px
		width 100px
		height 100px
		overflow hidden

@media (max-width 992px) and (orientation landscape)
	.landscape
		position fixed
		width 100%
		height 100%
		background #0e0e0e
		display flex
		justify-content center
		flex-direction column
		align-items center
		top 0
		left 0
		z-index 1000